<% @page_title = "New Admin User" %>

<%= link_to("<< Back to Search", root_path, :class => 'back-link') %>

<div class="admin-users new">
  <h2>Sign Up</h2>

  <%= form_for(@user) do |f| %>

    <%= render(:partial => 'form', :locals => {:f => f}) %>

    <div class="form-buttons">
      <%= f.submit("Create Account") %>
    </div>

  <% end %>
</div>
